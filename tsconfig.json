{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "es5",
      "es6"
    ],
    "target": "es2016",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "outDir": "dist"
  },
  "exclude": [
    "node_modules",
    "angular"
  ],
  "include": [
    "src/**/*"
  ]
}


